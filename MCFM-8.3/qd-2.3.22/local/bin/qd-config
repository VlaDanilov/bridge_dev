#!/bin/sh
# qd-config.  Generated from qd-config.in by configure.
prefix="/afs/desy.de/user/v/volinar/APPLgrid_development/MCFM-8.3/qd-2.3.22/local"
exec_prefix="${prefix}"
includedir="${prefix}/include"
libdir="${exec_prefix}/lib"
bindir="${exec_prefix}/bin"
datadir="${prefix}/share"
mandir="${datarootdir}/man"
sysconfdir="${prefix}/etc"
sharedstatedir="${prefix}/com"
srcdir="/afs/desy.de/user/v/volinar/APPLgrid_development/MCFM-8.3/qd-2.3.22"
builddir="/afs/desy.de/user/v/volinar/APPLgrid_development/MCFM-8.3/qd-2.3.22"
build_CXXFLAGS="-O2 "
build_LDFLAGS="-L/afs/desy.de/user/v/volinar/APPLgrid_development/Main/lib/:"
CXX="/cvmfs/sft.cern.ch/lcg/contrib/gcc/4.8/x86_64-slc6/bin/g++"
CXXFLAGS="-I$includedir "
FC="/cvmfs/sft.cern.ch/lcg/contrib/gcc/4.8/x86_64-slc6/bin/gfortran"

LDFLAGS="-L$libdir -lqd -lm -lrt "
STATICLDFLAGS="$libdir/libqd.a -lm -lrt "
LALDFLAGS="$libdir/libqd.la -lm -lrt "
SRC_LDFLAGS="$builddir/src/.libs/libqd.so -lm -lrt "
SRC_LALDFLAGS="$builddir/src/libqd.la -lm -lrt "
SRC_STATICLDFLAGS="$builddir/src/.libs/libqd.a -lm -lrt "

CFLAGS=" -I$includedir"
SRC_CFLAGS="-I$srcdir/src -I$builddir/src "

FCLIBS="-L$libdir -lqdmod -lqd -lm -lrt   -L/afs/desy.de/user/v/volinar/APPLgrid_development/Main/lib/: -L/cvmfs/sft.cern.ch/lcg/contrib/gcc/4.8.4/x86_64-slc6/bin/../lib/gcc/x86_64-unknown-linux-gnu/4.8.4 -L/cvmfs/sft.cern.ch/lcg/contrib/gcc/4.8.4/x86_64-slc6/bin/../lib/gcc -L/cvmfs/sft.cern.ch/lcg/contrib/gcc/4.8.4/x86_64-slc6/bin/../lib/gcc/x86_64-unknown-linux-gnu/4.8.4/../../../../lib64 -L/lib/../lib64 -L/usr/lib/../lib64 -L/cvmfs/sft.cern.ch/lcg/contrib/gcc/4.8.4/x86_64-slc6/bin/../lib/gcc/x86_64-unknown-linux-gnu/4.8.4/../../.. -lrt -lgfortran -lm -lquadmath "
FMAINLIB="-L$libdir -lqd_f_main"
FCFLAGS=""
MODFLAG="-I $includedir/qd"

build="@BUILDER@-@BUILDDATE@"
configure_args=" '--enable-fortran' '--enable-fma' '--prefix=/afs/desy.de/user/v/volinar/APPLgrid_development/MCFM-8.3/qd-2.3.22/local' 'CXX=/cvmfs/sft.cern.ch/lcg/contrib/gcc/4.8/x86_64-slc6/bin/g++' 'LDFLAGS=-L/afs/desy.de/user/v/volinar/APPLgrid_development/Main/lib/:' 'CC=/cvmfs/sft.cern.ch/lcg/contrib/gcc/4.8/x86_64-slc6/bin/gcc' 'FC=/cvmfs/sft.cern.ch/lcg/contrib/gcc/4.8/x86_64-slc6/bin/gfortran'"
from_source=0
usage()
{
	cat <<EOF
Usage: qd-config [OPTIONS]
Options:
	[--src]
	[--prefix]
	[--exec-prefix]
	[--version]
	[--libs-la]
	[--libs]
	[--fclibs]
	[--fcflags]
        [--fmainlib]
	[--cflags]
	[--cxx]
	[--cxxflags]
	[--build-flags]
	[--build-libs]
	[--configure-args]
EOF
	exit $1
}

while test $# -gt 0; do
  case "$1" in
  -*=*) optarg=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
  *) optarg= ;;
  esac

  case $1 in
    --src)
      from_source=1
      ;;
    --prefix)
      echo $prefix
      ;;
    --exec-prefix)
      echo $exec_prefix
      ;;
    --version)
      echo 2.3.22
      ;;
    --cflags)
      if test $from_source -eq 0; then
        echo $CFLAGS
      else
        echo $SRC_CFLAGS
      fi;
      ;;
    --libs-la)
      if test $from_source -eq 0; then
        echo $LALDFLAGS
      else
        echo $SRC_LALDFLAGS
      fi;
      ;;
    --libs)
      if test $from_source -eq 0; then
        echo $LDFLAGS
      else
        echo $SRC_LDFLAGS
      fi;
      ;;
    --static-libs)
      if test $from_source -eq 0; then
        echo $STATICLDFLAGS
      else
        echo $SRC_STATICLDFLAGS
      fi;
      ;;
    --build)
      echo $build
      ;;
    --build-flags)
      echo $build_CXXFLAGS
      ;;
    --build-libs)
      echo $build_LDFLAGS
      ;;
    --cxx)
      echo $CXX
      ;;
    --configure-args)
      echo $configure_args
      ;;
    --fclibs)
      echo $FCLIBS
      ;;
    --fcflags)
      echo "$MODFLAG $FCFLAGS"
      ;;
    --fmainlib)
      echo "$FMAINLIB"
      ;;
    --cxxflags)
      echo $CXXFLAGS
      ;;
    --fc)
      echo $FC
      ;;
    *)
      usage 1 1>&2
      ;;
  esac
  shift
done

